{
  "name": "online-fashion-store",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "lint": "eslint src --ext js --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src -d ./build/src",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && cross-env BUILD_MODE=prod node ./build/src/server.js",
    "dev": "cross-env BUILD_MODE=dev NODE_OPTIONS='--disable-warning=DEP0040' nodemon --exec babel-node  ./src/server.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.10",
    "@getbrevo/brevo": "^2.2.0",
    "add": "^2.0.6",
    "async-exit-hook": "^2.0.1",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "i": "^0.3.7",
    "joi": "^17.13.3",
    "joi-password-complexity": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.14.0",
    "ms": "^2.1.3",
    "npm": "^11.3.0",
    "prettier": "^3.5.3",
    "uuid": "^11.1.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.10",
    "@babel/node": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.47.0",
    "nodemon": "^3.0.1"
  }
}
